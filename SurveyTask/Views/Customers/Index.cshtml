            @Html.DisplayFor(modelItem => item.firstQ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecQ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.thirdQ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.forthQ)
        </td>
      
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
